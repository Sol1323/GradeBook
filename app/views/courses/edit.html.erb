<%= form_for @course, url: edit_course_path ,method: "put" do |f| %>
  <div>
    <%= f.text_field :name, placeholder: "Course Name" %>
  </div>
  <div>
    <%= f.text_field :number, placeholder: "Course Number" %>
  </div>
  <div>
    <%= f.hidden_field :teacher_id, :value => @teacher.id %>
  </div>
  <%= f.submit "Edit Course!", class:"waves-effect waves-light btn" %>
<% end %>

<br>
<div>
  Welcome, <%= @teacher.first_name %>!
</div>
<h1>My Classes!</h1>
<% @courses.each do |course| %>
<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey lighten-3">
      <div class="card-content white-text">
        <h5><%=link_to("#{course.name}", course_show_path(course.id)) %>
        <%= course.number %><br></h5>
      </div>
    </div>
  </div>
</div>
<% end %>
<br>
<%= link_to("New Course", new_course_path, class:"waves-effect waves-light btn")%> <br>
<%= link_to("Edit Profile", edit_teacher_path, class:"waves-effect waves-light btn")%><br>
<%= link_to("Delete Account", delete_user_path(current_user.id), class:"waves-effect waves-light red btn",:method => :delete, data: {confirm: "Are you sure you want to delete your account?"})%><br>
